{
    "name": "carbook-data-manager",
    "version": "0.0.30",
    "license": "MIT",
    "scripts": {
        "start": "ng serve",
        "prev:maj": "git commit -a -m \"Major commit\" ",
        "prev:min": "git commit -a -m \"Minor commit\" ",
        "prev:pat": "git commit -a -m \"Patch commit\"",
        "v:maj": "npm version major -m \"Bump Version to %s\"",
        "v:min": "npm version minor -m \"Bump Version to %s\"",
        "v:pat": "npm version patch -m \"Bump Version to %s\"",
        "postversion": "git push",
        "deploy:build": "ng build --base-href=/carbook-data-manager/ --environment=prod -oh all",
        "deploy:builddock": "cross-var docker build -t docker-reg.widas.de/carbook-data-manager:vP$npm_package_version-$(git rev-parse --short HEAD) .",
        "deploy:pushdock": "cross-var docker push docker-reg.widas.de/carbook-data-manager:vP$npm_package_version-$(git rev-parse --short HEAD)",
        "build": "npm run deploy:build && npm run deploy:builddock && npm run deploy:pushdock",
        "test:build": "ng build --base-href=/carbook-data-manager/ --environment=test -oh all",
        "test:builddock": "cross-var docker build -t docker-reg.widas.de/carbook-data-manager:v$npm_package_version-$(git rev-parse --short HEAD) .",
        "test:pushdock": "cross-var docker push docker-reg.widas.de/carbook-data-manager:v$npm_package_version-$(git rev-parse --short HEAD)",
        "test": "npm run test:build && npm run test:builddock && npm run test:pushdock",
        "latest": "git fetch && git pull origin development",
        "checkout:dev": "git checkout development"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "^1.0.0-beta.1",
        "@angular/animations": "^4.1.3",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/flex-layout": "^2.0.0-beta.8",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "@types/node": "^6.0.77",
        "angular2-toaster": "^4.0.0",
        "class-transformer": "^0.1.6",
        "core-js": "^2.4.1",
        "cross-var": "^1.0.3",
        "express": "^4.14.0",
        "fileapi": "^2.0.20",
        "moment": "^2.18.1",
        "ng2-toasty": "^4.0.3",
        "node-sass": "^4.9.0",
        "primeng": "^4.0.3",
        "rxjs": "^5.1.0",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular/cli": "1.0.0",
        "@angular/compiler-cli": "^4.0.0",
        "@types/googlemaps": "^3.26.20",
        "@types/jasmine": "2.5.38",
        "@types/node": "~6.0.60",
        "@types/moment": "^2.13.0",
        "codelyzer": "~2.0.0",
        "font-awesome": "^4.7.0",
        "jasmine-core": "~2.5.2",
        "jasmine-spec-reporter": "~3.2.0",
        "karma": "~1.4.1",
        "karma-chrome-launcher": "~2.0.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^0.2.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.0",
        "ts-node": "~2.0.0",
        "tslint": "~4.5.0",
        "typescript": "~2.2.0"
    }
}
